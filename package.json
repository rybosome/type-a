{
  "name": "@rybosome/type-a",
  "version": "0.10.0",
  "description": "Declarative class models with runtime validation for TypeScript",
  "keywords": [
    "typescript",
    "validation",
    "schema",
    "model",
    "class",
    "runtime"
  ],
  "author": "Ryan Eiger",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "tsup src/index.ts --watch --format esm,cjs",
    "build": "tsup src/index.ts --format esm,cjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "check": "pnpm typecheck && pnpm lint && pnpm format",
    "go": "tsx src/main.ts",
    "test": "pnpm check && pnpm build && vitest --run --coverage && pnpm docs:test",
    "docs:test": "tsx ./scripts/docs-test.ts",
    "docs:api": "typedoc --options typedoc.json",
    "size": "size-limit"
  },
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@eslint/js": "~9.30.1",
    "@types/node": "~22.16.0",
    "@typescript-eslint/eslint-plugin": "~8.36.0",
    "@typescript-eslint/parser": "~8.36.0",
    "eslint": "~9.30.1",
    "lz-string": "~1.5.0",
    "prettier": "~3.6.2",
    "tsup": "~8.5.0",
    "tsx": "~4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.36.0",
    "vitest": "~3.2.4",
    "typedoc": "~0.26.3",
    "size-limit": "~8.0.1",
    "@size-limit/esbuild": "~8.0.1",
    "@semantic-release/changelog": "~6.0.0",
    "@vitest/coverage-v8": "~3.2.4",
    "@size-limit/file": "~8.0.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
